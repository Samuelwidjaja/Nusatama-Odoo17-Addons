<!-- extend_account_invoice/views/account_move_view.xml -->
<odoo>
    <record id="view_move_form_inherit_sbu_jurnal" model="ir.ui.view">
        <field name="name">account.move.form.sbu_jurnal</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='partner_id']" position="after">
                <field name="sbu_jurnal"/>
                <!-- coba nambahin field cust po -->
                <field name="customer_po"/>
            </xpath>
        </field>
    </record>
</odoo>
